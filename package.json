{
  "name": "@nexpy/design-system",
  "description": "Nexpy design system.",
  "keywords": [
    "design system",
    "react",
    "typescript"
  ],
  "version": "2.0.3",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
